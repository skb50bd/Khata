{
  "Configuration":
  {
    "ConsoleTitle": "KHATA Console",
    "Serilog":
    {
      "MinimumLevel":
      {
        "Default": "Debug",
        "Override":
        {
          "System": "Information",
          "Microsoft": "Information"
        }
      },
      "WriteTo":
      [
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args":
          {
            "path": "logs\\log.txt",
            "rollingInterval": 3 // day
          }
        },
        {
          "Name": "Seq",
          "Args":
          {
            "serverUrl": "http://seq:5341/",
            "compact": true
          }
        }
      ],
      "Enrich":
      [
        "FromLogContext"
      ]
    }
  }
}