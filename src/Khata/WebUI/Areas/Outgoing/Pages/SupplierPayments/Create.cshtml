@page
@model SupplierPayments.CreateModel

@{
    ViewData["Title"] = "New Supplier Payment";
}

<div class="row">
    <div class="col-md-4">
        <form method="post" class="immutable-submit">
            <partial name="_SupplierPaymentForm" for="SupplierPaymentVm"/>
            
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary btn-lg" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/supplierPayment.js" asp-append-version="true"></script>
}
